/* MySQL specific comments */ SELECT 1
SELECT * FROM ident # MySQL comment
SELECT * FROM ident /*! MySQL command */
SELECT * FROM ident /*!50440 MySQL version command */

/* MySQL describe statements */ SELECT 1
DESCRIBE ident
DESCRIBE ident ident /* Describe a specific column */
DESCRIBE ident 'quote' /* Describe a specific column */
DESCRIBE ident 's%' /* Describe columns that match a regex */
DESCRIBE ident '___' /* Describe columns that match a regex */
DESC ident
DESC ident ident /* Describe a specific column */
DESC ident 'quote' /* Describe a specific column */
DESC ident 's%' /* Describe columns that match a regex */
DESC ident '___' /* Describe columns that match a regex */

/* MySQL SHOW TABLES statements */ SELECT 1
SHOW TABLES LIKE 'quote'
SHOW TABLES WHERE 2 = 1 + 1
SHOW TABLES FROM 'quote'
SHOW TABLES FROM 'quote' LIKE 'quote'
SHOW TABLES FROM 'quote' WHERE MD5('quote') = 'quote'
SHOW FULL TABLES LIKE 'quote'
SHOW FULL TABLES WHERE 2 = 1 + 1
SHOW FULL TABLES FROM 'quote'
SHOW FULL TABLES FROM 'quote' LIKE 'quote'
SHOW FULL TABLES FROM 'quote' WHERE MD5('quote') = 'quote'

/* MySQL SHOW TABLES statements */ SELECT 1
SHOW COLUMNS ident LIKE 'quote'
SHOW COLUMNS ident WHERE 1 = 1
SHOW FULL COLUMNS LIKE 'quote'
SHOW COLUMNS FROM ident LIKE 'quote'
SHOW COLUMNS FROM ident FROM ident LIKE 'quote'
SHOW COLUMNS FROM ident IN ident LIKE 'quote'
SHOW COLUMNS IN ident 'quote'
SHOW COLUMNS IN ident FROM ident LIKE 'quote'
SHOW COLUMNS IN ident IN ident 'quote'

/* MySQL SHOW CREATE DATABASE statements */ SELECT 1
SHOW CREATE DATABASE 'ident'
SHOW CREATE SCHEMA 'ident'

/* MySQL SHOW DATABASES statements */ SELECT 1
SHOW DATABASES LIKE 'ident'
SHOW SCHEMAS LIKE 'ident'
SHOW DATABASES WHERE 1 > 0
SHOW SCHEMAS LIKE 1 * 1 = 1

/* MySQL SHOW CREATE table statements */ SELECT 1
SHOW CREATE TABLE 'ident'

/* MySQL MATCH statements */ SELECT 1
SELECT * FROM ident WHERE MATCH (ident) AGAINST ('quote')
SELECT * FROM ident WHERE MATCH (ident, ident) AGAINST ('quote')
SELECT * FROM ident WHERE MATCH (ident, ident, ident) AGAINST ('quote')
SELECT * FROM ident WHERE MATCH (ident) AGAINST (CONCAT('quote', CHAR(5)))
SELECT * FROM ident WHERE MATCH (ident) AGAINST ('quote' IN BOOLEAN MODE)
SELECT * FROM ident WHERE MATCH (ident) AGAINST ('quote' WITH QUERY EXPANSION)

/* MySQL SET statements and global variables */ SELECT 1
SET max_join_size = DEFAULT
SET @@session.max_join_size = @@global.max_join_size
SELECT @@global.sql_mode, @@session.sql_mode, @@sql_mode
SET ONE_SHOT character_set_connections = latin5
SET ONE_SHOT collation_connection = latin5_turkish_ci
SHOW VARIABLES LIKE '%_connection'
SHOW GLOBAL VARIABLES LIKE '%_connection'
SET TRANSACTION ISOLATION LEVEL REPEATABLE READ
SET GLOBAL TRANSACTION ISOLATION LEVEL REPEATABLE READ
SET SESSION TRANSACTION ISOLATION LEVEL REPEATABLE READ
SET TRANSACTION ISOLATION LEVEL READ COMMITTED
SET GLOBAL TRANSACTION ISOLATION LEVEL READ COMMITTED
SET SESSION TRANSACTION ISOLATION LEVEL READ COMMITTED
SET TRANSACTION ISOLATION LEVEL REPEATABLE READ UNCOMMITTED
SET GLOBAL TRANSACTION ISOLATION LEVEL REPEATABLE READ UNCOMMITTED
SET SESSION TRANSACTION ISOLATION LEVEL REPEATABLE READ UNCOMMITTED
SET TRANSACTION ISOLATION LEVEL REPEATABLE SERIALIZABLE
SET GLOBAL TRANSACTION ISOLATION LEVEL REPEATABLE SERIALIZABLE
SET SESSION TRANSACTION ISOLATION LEVEL REPEATABLE SERIALIZABLE
SELECT @@GLOBAL.tx_isolation, @@tx_isolation
SET GLOBAL tx_isolation = 'REPEATABLE-READ'
SET SESSION tx_isolation = 'SERIALIZABLE'
SET NAMES utf8
