/* ? -> FROM */
SELECT 'quote' FROM ident
SELECT ident FROM ident
SELECT 1 FROM ident
SELECT 1.0 FROM ident
SELECT @local FROM ident
/* ident -> ident */
SELECT 'quote' FROM ident ident

